<resources>
    <style name="MyNavTextStyle" parent="TextAppearance.MaterialComponents.Body1">
        <item name="fontFamily">@font/sfpro_regu</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="MyButtonStyle" parent="@style/Widget.MaterialComponents.Button">
        <item name="android:backgroundTintMode">src_over</item>
        <item name="android:backgroundTint">@null</item>
    </style>
    <style name="CircleImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
    <style name="RoundedImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">12dp</item>
        <item name="cornerSizeTopLeft">20dp</item>
        <item name="cornerSizeTopRight">20dp</item>
        <item name="cornerSizeBottomLeft">20dp</item>
        <item name="cornerSizeBottomRight">20dp</item>
    </style>
</resources>